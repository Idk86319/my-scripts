-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BackgroundTransparency = 1.000
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.900677204, 0, 0.0646830499, 0)
Frame.Size = UDim2.new(0, 124, 0, 74)

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(223, 75, 255)
TextLabel.BorderSizePixel = 9
TextLabel.Position = UDim2.new(0, 0, 0.0135135138, 0)
TextLabel.Size = UDim2.new(0, 124, 0, 73)
TextLabel.Font = Enum.Font.Arcade
TextLabel.Text = "HI"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextScaled = true
TextLabel.TextSize = 14.000
TextLabel.TextStrokeColor3 = Color3.fromRGB(0, 49, 217)
TextLabel.TextStrokeTransparency = 0.000
TextLabel.TextWrapped = true

-- Scripts:

local function IRTZ_fake_script() -- TextLabel.fps script 
	local script = Instance.new('LocalScript', TextLabel)

	local Stats = game:GetService("Stats")
	local RunService = game:GetService("RunService")
	
	-- Reference to the existing TextLabel
	local textLabel = script.Parent
	assert(textLabel:IsA("TextLabel"), "Script must be inside a TextLabel")
	
	-- FPS calculation
	local frames = 0
	local lastTime = tick()
	local fps = 0
	
	RunService.RenderStepped:Connect(function()
		frames += 1
		local now = tick()
	
		if now - lastTime >= 1 then
			fps = frames
			frames = 0
			lastTime = now
		end
	end)
	
	-- Update text
	while true do
		task.wait(0.3)
	
		local pingStat = Stats.Network.ServerStatsItem["Data Ping"]
		local ping = math.floor(pingStat:GetValue())
	
		textLabel.Text = string.format(
			"FPS: %d\nPing: %d ",
			fps,
			ping
		)
	end
end
coroutine.wrap(IRTZ_fake_script)()
local function VVQNYVD_fake_script() -- TextLabel.drag script 
	local script = Instance.new('LocalScript', TextLabel)

	local frame = script.Parent
	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local dragging = false
	local dragStart = nil
	local startPos = nil
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
	
	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)
end
coroutine.wrap(VVQNYVD_fake_script)()
